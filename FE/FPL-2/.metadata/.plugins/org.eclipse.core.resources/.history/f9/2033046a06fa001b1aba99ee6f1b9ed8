#include <iostream>
using namespace std;

int main() {
	int num1=0,num2=0;

	cout<<"Enter the first no";
	cin>>num1;
	cout<<"Enter the second no";
	cin>>num2;

	cout << "Sum of two number is " << num1+num2 << endl;
	return 0;
}
